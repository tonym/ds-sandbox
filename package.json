      "optional": true
    },
    "@angular/platform-browser-dynamic": {
      "optional": true
    },
    "@angular/router": {
      "optional": true
    },
    "deepmerge": {
      "optional": true
    }
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.14",
    "@angular/cdk": "^11.0.0",
    "@angular/cli": "11.2.14",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/compiler-cli": "11.2.14",
    "@angular/core": "^11.0.0",
    "@angular/forms": "11.2.14",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@babel/core": "7.16.0",
    "@compodoc/compodoc": "1.1.11",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-a11y": "~6.3.0",
    "@storybook/addon-actions": "~6.3.0",
    "@storybook/addon-docs": "~6.3.0",
    "@storybook/addon-essentials": "~6.3.0",
    "@storybook/addon-links": "~6.3.0",
    "@storybook/addon-storysource": "~6.3.0",
    "@storybook/angular": "~6.3.0",
    "@types/angular": "1.8.2",
    "@types/deepmerge": "^2.2.0",
    "@types/jest": "24.9.1",
    "@types/node": "14.17.19",
    "@whitespace/storybook-addon-html": "5.0.0",
    "babel-loader": "8.2.3",
    "codesandbox": "2.2.3",
    "core-js": "3.21.1",
    "csstype": "~3.0.6",
    "deepmerge": "^4.2.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-junit": "10.0.0",
    "jest-preset-angular": "8.4.0",
    "lint-staged": "10.5.4",
    "markdown-loader-jest": "0.1.1",
    "minify-css-string": "1.0.0",
    "ng-packagr": "^11.0.0",
    "prettier": "2.6.1",
    "react-json-view": "1.21.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rxjs": "6.6.7",
    "storybook-addon-designs": "6.0.1",
    "ts-jest": "26.5.6",
    "tslib": "2.3.1",
    "typescript": "4.1.3",
    "typings": "2.1.1",
    "zone.js": "0.11.4"
  },
  "ngPackage": {
    "dest": "coreng",
    "lib": {
      "entryFile": "src/public-api.ts",
      "umdModuleIds": {
        "deepmerge": "deepmerge",
        "redux": "redux",
        "jss": "jss",
        "jss-preset-default": "preset"
      }
    },
    "allowedNonPeerDependencies": [
      "csstype",
      "deepmerge",
      "jss",
      "redux"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit-message && lint-staged"
    }
  },
  "scripts": {
    "build": "rimraf core && npm run build-angular && rollup -c && tsc && rimraf coreng",
    "build-angular": "ng-packagr -p package.json",
    "build:storybook": "npm run docs:json && npm run build-storybook",
    "build-storybook": "build-storybook -s public -o ./docs/latest",
    "docs:json": "compodoc -p ./tsconfig.dev.json -e json -d .",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "ngcopy": "",
    "postversion": "git push && git push --tags",
    "precommit-message": "echo Running pre-commit checks... && exit 0",
    "preversion": "npm run test",
    "storybook": "npm run docs:json && start-storybook -s ./public -p 6006 --no-manager-cache --no-version-updates",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --clearCache && jest --coverage --maxWorkers=50%",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage --collectCoverage=true",
    "terraform:format": "terraform fmt -recursive"
  }
}
