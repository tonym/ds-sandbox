    "redux": "^4.0.4"
  },
  "peerDependencies": {
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "deepmerge": "^4.2.2"
  },
  "peerDependenciesMeta": {
    "@angular/cdk": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    },
    "@angular/compiler": {
      "optional": true
    },
